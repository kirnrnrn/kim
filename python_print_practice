#문제5
a=int(input('정수를 입력하시오'))
b=int(input('정수를 입력하시오'))
c=int(input('정수를 입력하시오'))
if a>b and a>c:
    print('True')
elif a<=b and a<=c:
    print(False)
elif a = b and a = c and b = c:
    print('True')
else:
    print('False')

# 문제6
a=int(input('정수를 입력하시오'))
b=int(input('정수를 입력하시오'))
print(a,'/',b,'=',a/b,'...',a%b)

#문제7
width=int(input('가로의 길이를 입력하시오'))
lenght=int(input('세로의 길이를 입력하시오'))
w=width+5
l=lenght*2
print('width','=',w)
print('lenght','=',l)
print('area','=',w*l)

#문제8
minh=int(input('민수의 키를 입력하시오'))
minw=int(input('민수의 몸무게를 입력하시오'))
kah=int(input('가영이의 키를 입력하시오'))
kaw=int(input('가영이의 몸무게를 입력하시오'))
if minh>kah and minw>kaw:
    print('True')
else:
    print('False')
