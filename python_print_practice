#문제1
kor=input('국어 점수를 입력하시오')
mat=input('수학 점수를 입력하시오')
eng=input('영어 점수를 입력하시오')
k=int(kor)
m=int(mat)
e=int(eng)
sum= k+m+e
avg=sum/3
print('kor',kor)
print('mat',mat)
print('eng',eng)
print('sum',sum)
print('avg',avg)

